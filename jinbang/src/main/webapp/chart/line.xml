<anychart>
	<charts>
		<chart plot_type="CategorizedVertical">
			<chart_settings>
			<!-- 
			<chart_background enabled="false">
				<inside_margin all="0"/>
				<corners type="Square"/>
			</chart_background>
			 -->
				<title enabled="true">
					<text>
						<![CDATA[24小时业务变化量]]>
					</text>
				</title>
				<legend enabled="true">
					<title enabled="false" />
					<icon>
						<marker enabled="true" type="%MarkerType" size="8" />
					</icon>
				</legend>
				<axes>
					<x_axis tickmarks_placement="Center">
						<title>
							<text>
								<![CDATA[ 时间点 ]]>
							</text>
						</title>
					</x_axis>
					<y_axis>
						<title>
							<text>
								<![CDATA[ 业务量(百条/分) ]]>
							</text>
						</title>
						<labels>
							<format>
								<![CDATA[ {%Value}{numDecimals:0} ]]>
							</format>
						</labels>
						<minor_grid enabled="false" />
						<major_grid interlaced="false" />
						<minor_tickmark enabled="false" />
					</y_axis>
				</axes>
			</chart_settings>
			<data_plot_settings default_series_type="Line">
				<line_series>
					<line_style>
						<line dashed="false"/>
					</line_style>
					<effects enabled="false" />
				</line_series>
			</data_plot_settings>
			<data>
				<series name="语音">
